config_version=5

[application]

config/name="mini_battler"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Global="*res://autoloads/Global.gd"
DamageNumbers="*res://autoloads/damageNumbers.gd"
LogrosData="*res://logros/data_logros/data_logros.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=960

[editor_plugins]

enabled=PackedStringArray("res://addons/script-ide/plugin.cfg")

[file_customization]

folder_colors={
"res://character/": "orange",
"res://enemies/": "red",
"res://logros/": "pink",
"res://managers/": "yellow",
"res://proyectiles/": "green",
"res://resources/": "purple",
"res://ui/": "green"
}

[global_group]

player=""
enemigos=""

[gui]

theme/default_font_multichannel_signed_distance_field=true
theme/default_font_generate_mipmaps=true

